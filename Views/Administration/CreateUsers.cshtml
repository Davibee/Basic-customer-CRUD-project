

@model V3.ViewModels.CreateUserViewModel

@using (Html.BeginForm("CreateUsers", "Administration"))
{
    <h1>Add new user</h1>


    <div class="form-group">
        @Html.ValidationSummary()
        <label>Name</label>
        @Html.TextBoxFor(r => r.UserName, new { @class = "form-control" })
        @Html.ValidationMessageFor(r => r.UserName)

    </div>
    

    <button type="submit" class="btn btn-primary">Save</button>
}